{
  "name": "xinhuo-enterprise-website",
  "version": "2.0.0",
  "description": "新欣火企业级官网系统 - Vue3现代化版本",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "start": "node tools/build-scripts/server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit",
    "deploy": "node tools/deployment/deploy.js"
  },
  "keywords": [
    "vue3",
    "typescript",
    "vite",
    "enterprise",
    "education",
    "xinhuo"
  ],
  "author": "新欣火教育",
  "license": "MIT",
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.0",
    "axios": "^1.6.0",
    "canvas-scroll-clip": "^1.3.2",
    "dayjs": "^1.11.0",
    "element-plus": "^2.4.0",
    "lodash-es": "^4.17.0",
    "pinia": "^2.1.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/ui": "^1.0.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.0",
    "jsdom": "^23.0.0",
    "prettier": "^3.1.0",
    "sass": "^1.93.2",
    "sass-embedded": "^1.93.2",
    "typescript": "^5.3.0",
    "unplugin-auto-import": "^0.17.0",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^2.0.0"
  }
}
